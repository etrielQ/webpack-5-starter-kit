extends ../layout/layoutMain
block append vars
  - var header = false 
  - var footer = false
  - var pageName = "Uikit"

block append head
  title= pageName + " Webpack 5 Starter Kit"
block append styles 
  link(rel="stylesheet" href="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.5.1/build/styles/default.min.css")

block content
  main#swup.transition-fade
    section.uikit
      .uikit__side
        .uikit__side-logo 
          img(src="../../images/base/babel-logo-primary.svg", alt="") 
        .uikit__side-search 
          input.js-uikit-search(type="text", placeholder="Search...")
          .uikit__side-version Babel Uikit v1.1

        .uikit__side-wrapper.js-uikit-scroll.js-uikit-side
          .uikit__side-field 
            .uikit__side-title.txt.txt--px18.txt--font600 getting started
            ul.uikit__side-list
              
          .uikit__side-field 
            .uikit__side-title.txt.txt--px18.txt--font600 base
            ul.uikit__side-list


          .uikit__side-field 
            .uikit__side-title.txt.txt--px18.txt--font600 components
            ul.uikit__side-list

              
      .uikit__content 
        .container 
          .uikit__header 
            .uikit__header-title.txt.txt--px24.txt--font600.js-scroll-text Overview
            .uikit__header-right 
              a(href="./images/images.html" target="_blank").uikit__header-btn 
                | Images
              a(href="./icons/icons-sprite.html" target="_blank").uikit__header-btn 
                | Icons
              .uikit__header-btn.uikit__header-btn--dark
                | Website
          .uikit__wrapper.js-uikit-content
            .uikit__item(data-ui-parent="getting started")#overview
              .uikit__item-message
              .uikit-overview 
                .uikit-overview__title Hello,
                  span Visitor 
                .uikit-overview__info This page is a Uikit page that we use to develop the frontend.
                .uikit-overview__txt If you encounter a problem during development, please ignore it until the final version is released.
                .uikit-overview__copyright 
                  p Being Developed by 
                    a(href="https://www.babel.com.tr" target="_blank") Babel
                  strong all rights reserved.
            .uikit__item(data-ui-parent="getting started")#build
              p Aşağıdaki github reposundan projeyi klonlayarak indirme işlemini yapabilirsiniz.
              pre.mb-4
                code(class="language-html")
                  | https://github.com/etrielQ/webpack-5-starter-kit.git
              p Sırasıyla Adımları Takip edin.
              pre.mb-4
                code(class="language-html")
                  | npm install
              p Eğer hata alırsanız;
              pre.mb-4
                code(class="language-html")
                  | npm install --legacy-peer-deps
              p
                strong Kurulum Tamam. 
                |Aşağıdaki komutu çalıştırarak projeyi çalıştırabilirsiniz.
              pre.mb-4
                code(class="language-html")
                  | npm run serve
              p Son olarak Build çıktısı almak için aşağıdaki komutu uygulayabilirsiniz. 
              pre
                code.language-html
                  | npm run build
            .uikit__item(data-ui-parent="getting started")#file-structure
              ul.uikit-filetree 
